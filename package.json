{
  "name": "tink-connect-example",
  "version": "1.0.0",
  "description": "An example app showing how to use the Tink API in a third-party app.",
  "repository": "github:tink-ab/tink-connect-example",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "client": "cd client && npm start",
    "server": "node server.js",
    "dev": "concurrently --kill-others-on-fail \"nodemon server.js\" \"npm run client\"",
    "postinstall": "cd client && npm install",
    "heroku-postbuild": "cd app && cd client && npm install --only=dev && npm install && npm run build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "1.19.0",
    "concurrently": "5.1.0",
    "express": "4.17.1",
    "node-fetch": "2.6.1"
  },
  "devDependencies": {
    "nodemon": "2.0.2"
  }
}
